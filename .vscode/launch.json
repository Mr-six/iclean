{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "启动程序",
			"program": "${workspaceRoot}/index.js",
			"console": "integratedTerminal",
			"protocol": "inspector",
			"env": {
				"NODE_ENV": "development"
			}
		},
		{
			"type": "node",
			"request": "attach",
			"name": "附加到进程",
			"protocol": "inspector",
			"restart": true,
			"port": 9229,
			"address": "localhost",
			"sourceMaps": false,
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		}
	],
		"compounds": []
}